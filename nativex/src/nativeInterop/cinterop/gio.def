headers = gio/gio.h
package = gio
headerFilter = gobject/* gio/* gdk/* glib/* gdk-pixbuf/* atk/* cairo/*

# pkg-config --cflags gtk+-3.0
compilerOpts.linux = -pthread -I/usr/include/libmount -I/usr/include/blkid -I/usr/include/glib-2.0 -I/usr/lib/x86_64-linux-gnu/glib-2.0/include

# pkg-config --libs gtk+-3.0
linkerOpts.linux = -lgio-2.0 -lgobject-2.0 -lglib-2.0